[{"text": "Resolution 2772 (2025)", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Adopted by the Security Council at its 9860th meeting, on 17 February 2025", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Security Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Recalling its previous resolutions concerning Sudan, in particular 1591 (2005), 1651 (2005), 1665 (2006), 1672 (2006), 1713 (2006), 1779 (2007), 1841 (2008), 1891 (2009), 1945 (2010), 1982 (2011), 2035 (2012), 2091 (2013), 2138 (2014), 2200 (2015), 2265 (2016), 2340 (2017), 2400 (2018), 2455 (2019), 2508 (2020), 2562 (2021), 2620 (2022), supplemented by 2664 (2022), 2676 (2023), 2725 (2024), and 2750 (2024), its Presidential Statement of 11 December 2018 (S/PRST/2018/19), and its press statements,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": [["1591 (2005)", "https://docs.un.org/en/S/RES/1591(2005)"], ["1651 (2005)", "https://docs.un.org/en/S/RES/1651(2005)"], ["1665 (2006)", "https://docs.un.org/en/S/RES/1665(2006)"], ["1672 (2006)", "https://docs.un.org/en/S/RES/1672(2006)"], ["1713 (2006)", "https://docs.un.org/en/S/RES/1713(2006)"], ["1779 (2007)", "https://docs.un.org/en/S/RES/1779(2007)"], ["1841 (2008)", "https://docs.un.org/en/S/RES/1841(2008)"], ["1891 (2009)", "https://docs.un.org/en/S/RES/1891(2009)"], ["1945 (2010)", "https://docs.un.org/en/S/RES/1945(2010)"], ["1982 (2011)", "https://docs.un.org/en/S/RES/1982(2011)"], ["2035 (2012)", "https://docs.un.org/en/S/RES/2035(2012)"], ["2091 (2013)", "https://docs.un.org/en/S/RES/2091(2013)"], ["2138 (2014)", "https://docs.un.org/en/S/RES/2138(2014)"], ["2200 (2015)", "https://docs.un.org/en/S/RES/2200(2015)"], ["2265 (2016)", "https://docs.un.org/en/S/RES/2265(2016)"], ["2340 (2017)", "https://docs.un.org/en/S/RES/2340(2017)"], ["2400 (2018)", "https://docs.un.org/en/S/RES/2400(2018)"], ["2455 (2019)", "https://docs.un.org/en/S/RES/2455(2019)"], ["2508 (2020)", "https://docs.un.org/en/S/RES/2508(2020)"], ["2562 (2021)", "https://docs.un.org/en/S/RES/2562(2021)"], ["2620 (2022)", "https://docs.un.org/en/S/RES/2620(2022)"], ["2664 (2022)", "https://docs.un.org/en/S/RES/2664(2022)"], ["2676 (2023)", "https://docs.un.org/en/S/RES/2676(2023)"], ["2725 (2024)", "https://docs.un.org/en/S/RES/2725(2024)"], ["2750 (2024)", "https://docs.un.org/en/S/RES/2750(2024)"], ["S/PRST/2018/19", "https://docs.un.org/en/S/PRST/2018/19"]]}, {"text": "Determining that the situation in Sudan continues to constitute a threat to international peace and security in the region,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Determining", "action_verb_type": "preambular", "action_verb_normalized": "determin", "links": []}, {"text": "Acting under Chapter VII of the Charter of the United Nations,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Acting", "action_verb_type": "preambular", "action_verb_normalized": "act", "links": []}, {"text": "Decides to extend until 12 March 2026 the mandate of the Panel of Experts originally appointed pursuant to resolution 1591 (2005) and previously extended by resolutions 1779 (2007), 1841 (2008), 1945 (2010), 2035 (2012), 2138 (2014), 2200 (2015), 2265 (2016), 2340 (2017), 2400 (2018), 2455 (2019), 2508 (2020), 2562 (2021), 2620 (2022), 2676 (2023), and 2725 (2024), reaffirms the mandate of the Panel of Experts as established in these resolutions, and requests the Panel of Experts to provide the Security Council Committee established pursuant to resolution 1591 (2005) concerning the Sudan (hereafter “the Committee”) with an interim report on its activities no later than 12 August 2025, and provide to the Council, after discussion with the Committee, a final report by 13 January 2026 with its findings and recommendations, and further requests the Panel of Experts to provide updates every three months to the Committee regarding its activities, including Panel travel, and the implementation and effectiveness of paragraph 10 of resolution 1945 (2010), and expresses its intention to review the mandate and take appropriate action regarding the further extension of the mandate no later than 12 February 2026; and encourages all parties and all Member States, as well as international, regional and subregional organizations, to ensure continued cooperation with the Panel of Experts and the safety of its members;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": [["1591 (2005)", "https://docs.un.org/en/S/RES/1591(2005)"], ["1779 (2007)", "https://docs.un.org/en/S/RES/1779(2007)"], ["1841 (2008)", "https://docs.un.org/en/S/RES/1841(2008)"], ["1945 (2010)", "https://docs.un.org/en/S/RES/1945(2010)"], ["2035 (2012)", "https://docs.un.org/en/S/RES/2035(2012)"], ["2138 (2014)", "https://docs.un.org/en/S/RES/2138(2014)"], ["2200 (2015)", "https://docs.un.org/en/S/RES/2200(2015)"], ["2265 (2016)", "https://docs.un.org/en/S/RES/2265(2016)"], ["2340 (2017)", "https://docs.un.org/en/S/RES/2340(2017)"], ["2400 (2018)", "https://docs.un.org/en/S/RES/2400(2018)"], ["2455 (2019)", "https://docs.un.org/en/S/RES/2455(2019)"], ["2508 (2020)", "https://docs.un.org/en/S/RES/2508(2020)"], ["2562 (2021)", "https://docs.un.org/en/S/RES/2562(2021)"], ["2620 (2022)", "https://docs.un.org/en/S/RES/2620(2022)"], ["2676 (2023)", "https://docs.un.org/en/S/RES/2676(2023)"], ["2725 (2024)", "https://docs.un.org/en/S/RES/2725(2024)"], ["1591 (2005)", "https://docs.un.org/en/S/RES/1591(2005)"], ["1945 (2010)", "https://docs.un.org/en/S/RES/1945(2010)"]]}, {"text": "Decides to remain seized of the matter.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}]