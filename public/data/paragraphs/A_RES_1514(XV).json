[{"text": "<!-- Page 1 -->", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "66", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "General Assembly — Fifteenth Session", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "should be admitted to membership in the United Nations.81", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Having considered the application for membership of the Republic of Mali,82", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Decides to admit the Republic of Mali to membership in the United Nations.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "876th plenary meeting,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "28 September 1960.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1492 (XV). Admission of the Federation of Nigeria to membership in the United Nations", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Having received the recommendation of the Security Council of 7 October 1960 that the Federation of Nigeria should be admitted to membership in the United Nations,83", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Having considered the application for membership of the Federation of Nigeria,84", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Decides to admit the Federation of Nigeria to membership in the United Nations.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "893rd plenary meeting,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "7 October 1960.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1495 (XV). Co-operation of Member States", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Deeply concerned by the increase in world tensions,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Considering that the deterioration in international relations constitutes a grave risk to world peace and co-operation,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Conscious that both in the General Assembly and in the world at large it is necessary to arrest this trend in international relations and to contribute towards greater harmony among nations irrespective of the differences in their political and economic systems,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1. Urges that all countries, in accordance with the Charter of the United Nations, refrain from actions likely to aggravate international tensions;", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "2. Reaffirms the conviction that the strength of the United Nations rests on the co-operation of its Member States which should be forthcoming in full measure so that the Organization becomes a more effective instrument for the safeguarding of peace and for the promotion of the economic and social advancement of all peoples;", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "3. Urges further that immediate and constructive steps should be adopted in regard to the urgent problems concerning the peace of the world and the advancement of its peoples;", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "4. Appeals to all Member States to use their utmost endeavours to these ends.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "907th plenary meeting,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "17 October 1960.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1503 (XV). Report of the International Atomic Energy Agency", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Takes note of the report of the International Atomic Energy Agency to the General Assembly for the year 1959-1960.35", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "943rd plenary meeting,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "12 December 1960.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1513 (XV). Report of the Security Council", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Takes note of the report of the Security Council to the General Assembly covering the period from 16 July 1959 to 15 July 1960.36", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "943rd plenary meeting,", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "12 December 1960.", "type": "frontmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1514 (XV). Declaration on the granting of independence to colonial countries and peoples", "type": "title", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Mindful of the determination proclaimed by the peoples of the world in the Charter of the United Nations to reaffirm faith in fundamental human rights, in the dignity and worth of the human person, in the equal rights of men and women and of nations large and small and to promote social progress and better standards of life in larger freedom,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Conscious of the need for the creation of conditions of stability and well-being and peaceful and friendly relations based on respect for the principles of equal rights and self-determination of all peoples, and of universal respect for, and observance of, human rights and fundamental freedoms for all without distinction as to race, sex, language or religion,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Conscious", "action_verb_type": "operative", "action_verb_normalized": null, "links": []}, {"text": "Recognizing the passionate yearning for freedom in all dependent peoples and the decisive role of such peoples in the attainment of their independence,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recognizing", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Aware of the increasing conflicts resulting from the denial of or impediments in the way of the freedom of such peoples, which constitute a serious threat to world peace,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Considering the important role of the United Nations in assisting the movement for independence in Trust and Non-Self-Governing Territories,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Considering", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Recognizing that the peoples of the world ardently desire the end of colonialism in all its manifestations,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recognizing", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Convinced that the continued existence of colonialism prevents the development of international economic cooperation, impedes the social, cultural and economic development of dependent peoples and militates against the United Nations ideal of universal peace,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Convinced", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Affirming that peoples may, for their own ends, freely dispose of their natural wealth and resources without prejudice to any obligations arising out of international economic co-operation, based upon the principle of mutual benefit, and international law,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Affirming", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Believing that the process of liberation is irresistible and irreversible and that, in order to avoid serious crises, an end must be put to colonialism and all practices of segregation and discrimination associated therewith,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Believing", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Welcoming the emergence in recent years of a large number of dependent territories into freedom and independence, and recognizing the increasingly powerful trends towards freedom in such territories which have not yet attained independence,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Welcoming", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "---", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "81 Ibid., document A/4514.", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "82 Ibid., document A/4512.", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "83 Ibid., document A/4533.", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "84 Ibid., document A/4527.", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "85 Annual report of the Board of Governors to the General Conference, 1 July 1959-30 June 1960, Vienna, July 1960 (A/4531 and Corr.1 and Add.1).", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "36 Official Records of the General Assembly, Fifteenth Session, Supplement No. 2 (A/4494).", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "<!-- Page 2 -->", "type": "footer", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Resolutions adopted without reference to a Committee 67", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Resolutions", "action_verb_type": "operative", "action_verb_normalized": null, "links": []}, {"text": "Convinced that all peoples have an inalienable right to complete freedom, the exercise of their sovereignty and the integrity of their national territory,", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Convinced", "action_verb_type": "preambular", "action_verb_normalized": null, "links": []}, {"text": "Solemnly proclaims the necessity of bringing to a speedy and unconditional end colonialism in all its forms and manifestations;", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "And to this end", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Declares that:", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Declares", "action_verb_type": "operative", "action_verb_normalized": null, "links": []}, {"text": "The subjection of peoples to alien subjugation, domination and exploitation constitutes a denial of fundamental human rights, is contrary to the Charter of the United Nations and is an impediment to the promotion of world peace and co-operation.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "All peoples have the right to self-determination; by virtue of that right they freely determine their political status and freely pursue their economic, social and cultural development.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Inadequacy of political, economic, social or educational preparedness should never serve as a pretext for delaying independence.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "All armed action or repressive measures of all kinds directed against dependent peoples shall cease in order to enable them to exercise peacefully and freely their right to complete independence, and the integrity of their national territory shall be respected.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "4.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Immediate steps shall be taken, in Trust and Non-Self-Governing Territories or all other territories which have not yet attained independence, to transfer all powers to the peoples of those territories, without any conditions or reservations, in accordance with their freely expressed will and desire, without any distinction as to race, creed or colour, in order to enable them to enjoy complete independence and freedom.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "5.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Any attempt aimed at the partial or total disruption of the national unity and the territorial integrity of a country is incompatible with the purposes and principles of the Charter of the United Nations.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "6.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "All States shall observe faithfully and strictly the provisions of the Charter of the United Nations, the Universal Declaration of Human Rights and the present Declaration on the basis of equality, non-interference in the internal affairs of all States, and respect for the sovereign rights of all peoples and their territorial integrity.", "type": "paragraph", "language": "english", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "7.", "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "947th plenary meeting,", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "14 December 1960.", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "1592 (XV). The situation in the Republic of the Congo", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Having considered the item entitled “The situation in the Republic of the Congo”,", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Noting that the previous resolutions of the Security Council and the General Assembly on this subject are still in effect,", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Decides to keep this item on the agenda of its resumed fifteenth session.", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "958th plenary meeting,", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "20 December 1960.", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "  ", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Note", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Appointment of the Peace Observation Commission (item 18)", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "At its 960th plenary meeting on 20 December 1960, the General Assembly decided to reappoint, for the calendar years 1961 and 1962, the present members of the Peace Observation Commission. The Commission is therefore composed as follows: China, Czechoslovakia, France, Honduras, India, Iraq, Israel, New Zealand, Pakistan, Sweden, Union of Soviet Socialist Republics, United Kingdom of Great Britain and Northern Ireland, United States of America and Uruguay.", "type": "backmatter", "language": "english", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}]