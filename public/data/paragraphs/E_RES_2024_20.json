[{"text": "2024 session", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Agenda item 12 (e)", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Coordination, programme and other questions: long-term programme of support for Haiti", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Resolution adopted by the Economic and Social Council on 23 July 2024", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "[on a proposal considered in plenary meeting (E/2024/L.25)]", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": [["E/2024/L.25", "https://undocs.org/en/E/2024/L.25"]]}, {"text": "2024/20.\tAd Hoc Advisory Group on Haiti", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Economic and Social Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Recalling its resolutions 2004/52 of 23 July 2004, 2005/46 of 27 July 2005, 2006/10 of 26 July 2006, 2007/13 of 25 July 2007, 2008/10 of 23 July 2008, 2009/4 of 23 July 2009, 2010/28 of 23 July 2010, 2012/21 of 26 July 2012, 2013/15 of 23 July 2013, 2014/37 of 18 November 2014, 2015/18 of 21 July 2015, 2016/28 of 27 July 2016, 2017/26 of 25 July 2017, 2018/19 of 24 July 2018, 2019/32 of 24 July 2019, 2020/11 of 17 July 2020, 2021/18 of 21 July 2021, 2022/20 of 22 July 2022 and 2023/32 of 26 July 2023 and its decisions 2004/322 of 11 November 2004, 2009/211 of 20 April 2009, 2009/267 of 15 December 2009, 2011/207 of 17 February 2011, 2011/211 of 26 April 2011, 2011/268 of 28 July 2011, 2013/209 of 15 February 2013, 2014/207 of 30 January 2014, 2014/210 of 23 April 2014, 2014/221 of 13 June 2014, 2017/214 of 19 April 2017, 2021/238 of 9 June 2021, 2022/314 of 16 February 2022, 2023/201 A of 31 October 2022, 2023/201 B of 7 December 2022, 2023/201 C of 5 April 2023, 2023/201 D of 26 July 2023 and 2024/222 of 5 June 2024,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": [["2012/21", "https://undocs.org/en/E/RES/2012/21"], ["2013/15", "https://undocs.org/en/E/RES/2013/15"], ["2014/37", "https://undocs.org/en/E/RES/2014/37"], ["2015/18", "https://undocs.org/en/E/RES/2015/18"], ["2016/28", "https://undocs.org/en/E/RES/2016/28"], ["2017/26", "https://undocs.org/en/E/RES/2017/26"], ["2019/32", "https://undocs.org/en/E/RES/2019/32"], ["2020/11", "https://undocs.org/en/E/RES/2020/11"], ["2021/18", "https://undocs.org/en/E/RES/2021/18"], ["2022/20", "https://undocs.org/en/E/RES/2022/20"], ["2023/32", "https://undocs.org/en/E/RES/2023/32"]]}, {"text": "Deeply concerned about the complex and multifaceted crisis in Haiti, and underlining high levels of food insecurity and malnutrition as well as the resurgence of cholera,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "concerned", "action_verb_type": "preambular", "action_verb_normalized": "concerned", "links": []}, {"text": "Recognizing the need for urgent measures to restore security by addressing the structural causes of violence, such as inequalities, extreme poverty, high unemployment, human rights violations, impunity, corruption and weak public institutions, and by creating favourable conditions for the re-establishment of democratic institutions,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recognizing", "action_verb_type": "preambular", "action_verb_normalized": "recogniz", "links": []}, {"text": "Emphasizing the urgent need to fully fund the appeal of 673.8 million United States dollars made by the United Nations and partners, set out in the 2024 Humanitarian Needs and Response Plan for Haiti, to address the most immediate humanitarian and protection needs, including food security, emergency livelihood assistance and investment in the agricultural sector to safeguard the resilience of the most vulnerable Haitians,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Emphasizing", "action_verb_type": "preambular", "action_verb_normalized": "emphasiz", "links": []}, {"text": "Welcomes the report of the Ad Hoc Advisory Group on Haiti and its focus on the humanitarian situation and long-term socioeconomic development of Haiti;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Welcomes", "action_verb_type": "operative", "action_verb_normalized": "welcom", "links": []}, {"text": "Decides to extend the mandate of the Ad Hoc Advisory Group on Haiti until the conclusion of the 2025 session, with the purpose of following closely and working together with Haitian authorities on the long-term development strategy of Haiti to promote socioeconomic recovery, reconstruction and institutional stability, with particular attention to the need to ensure coherence and sustainability in international support for Haiti, based on the long-term national development priorities of Haiti, and stressing the need to avoid overlap and duplication with respect to existing mechanisms;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}, {"text": "Requests the Ad Hoc Advisory Group on Haiti to submit a report on its work, with recommendations, as appropriate, to the Economic and Social Council for its consideration at its 2025 session.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "37th plenary meeting", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "23 July 2024", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}]