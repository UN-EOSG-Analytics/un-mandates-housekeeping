[{"text": "Resolution 2636 (2022)", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Adopted by the Security Council at its 9054th meeting, on 3 June 2022", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Security Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Reaffirming all its previous resolutions and presidential statements concerning the situation in Sudan,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": []}, {"text": "Reaffirming its strong commitment to the sovereignty, unity, independence, and territorial integrity of Sudan,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": []}, {"text": "Taking note of the Report of the Secretary-General of the United Nations on the situation in Sudan and the activities of the United Nations Integrated Transition Assistance Mission (UNITAMS) (S/2022/400),", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Taking", "action_verb_type": "preambular", "action_verb_normalized": "tak", "links": [["S/2022/400", "https://undocs.org/en/S/2022/400"]]}, {"text": "Decides to extend the mandate of UNITAMS until 3 June 2023 as contained in paragraph 3 of resolution 2579 (2021);", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": [["2579 (2021)", "https://undocs.org/en/S/RES/2579(2021)"]]}, {"text": "Requests the Secretary-General to continue to report to the Security Council every 90 days on the implementation of UNITAMS mandate;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "Decides to remain actively seized of the matter.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}]