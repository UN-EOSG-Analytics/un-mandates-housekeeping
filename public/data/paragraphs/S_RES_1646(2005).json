[{"text": "Resolution 1646 (2005)", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Adopted by the Security Council at its 5335th meeting, \non 20 December 2005", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Security Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Recalling resolution 1645 (2005),", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": []}, {"text": "Decides pursuant to paragraph 4 (a) of resolution 1645 (2005) that the permanent members listed in article 23 (1) of the Charter shall be members of the Organizational Committee of the Peacebuilding Commission and that, in addition, the Council shall select annually two of its elected members to participate in the Organizational Committee;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}, {"text": "Decides that the annual report referred to in paragraph 15 of resolution 1645 (2005) shall also be submitted to the Security Council for an annual debate.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}]