[{"text": "Resolution adopted by the General Assembly on 19 December 2011", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "[on the report of the Third Committee (A/66/455 and Corr.1)]", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "66/131.\tConvention on the Elimination of All Forms \nof Discrimination against Women", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Recalling its resolution 64/138 of 18 December 2009,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": []}, {"text": "Welcomes the report of the Secretary-General on the status of the Convention on the Elimination of All Forms of Discrimination against Women;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Welcomes", "action_verb_type": "operative", "action_verb_normalized": "welcom", "links": []}, {"text": "Also welcomes the reports of the Committee on the Elimination of Discrimination against Women on its forty-fourth and forty-fifth and forty-sixth to forty-eighth sessions;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "welcomes", "action_verb_type": "operative", "action_verb_normalized": "welcom", "links": []}, {"text": "Invites the Chair of the Committee on the Elimination of Discrimination against Women to address and engage in an interactive dialogue with the General Assembly at its sixty-seventh and sixty-eighth sessions under the item on the advancement of women;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Invites", "action_verb_type": "operative", "action_verb_normalized": "invit", "links": []}, {"text": "Requests the Secretary-General to submit to the General Assembly at its sixty-eighth session a report on the status of the Convention.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "4.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "89th plenary meeting\n19 December 2011", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}]