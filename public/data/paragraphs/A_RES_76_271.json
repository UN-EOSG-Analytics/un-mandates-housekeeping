[{"text": "Resolution adopted by the General Assembly on 29 June 2022", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "[on the report of the Fifth Committee (A/76/633/Add.2, para. 6)]", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": [["A/76/633/Add.2", "https://undocs.org/en/A/76/633/Add.2"]]}, {"text": "76/271.\tAddressing racism and promoting dignity for all in the United Nations Secretariat", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The General Assembly,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Having considered the report of the Secretary-General and the related report of the Advisory Committee on Administrative and Budgetary Questions,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Having considered", "action_verb_type": "preambular", "action_verb_normalized": "having considered", "links": []}, {"text": "Takes note of the report of the Secretary-General;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Takes", "action_verb_type": "operative", "action_verb_normalized": "tak", "links": []}, {"text": "Endorses the conclusions and recommendations contained in the report of the Advisory Committee on Administrative and Budgetary Questions;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Endorses", "action_verb_type": "operative", "action_verb_normalized": "endors", "links": []}, {"text": "Approves the establishment of eight new general temporary assistance positions for 18 months, as detailed in the table below:", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Approves", "action_verb_type": "operative", "action_verb_normalized": "approv", "links": []}, {"text": "Abbreviations: GS (OL), General Service (Other level); GS (PL), General Service (Principal level).", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Appropriates an additional amount of 1,194,200 United States dollars, comprising 861,400 dollars under section 29A, Department of Management Strategy, Policy and Compliance, and 332,800 dollars under section 29B, Department of Operational Support, of the programme budget for 2022;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "4.", "action_verb": "Appropriates", "action_verb_type": "operative", "action_verb_normalized": "appropriat", "links": []}, {"text": "Also appropriates an amount of 55,700 dollars under section 36, Staff assessment, to be offset by an equivalent amount under income section 1, Income from staff assessment, of the programme budget for 2022.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "5.", "action_verb": "appropriates", "action_verb_type": "operative", "action_verb_normalized": "appropriat", "links": []}, {"text": "89th plenary meeting", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": nul