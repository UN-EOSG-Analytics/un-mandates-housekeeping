[{"text": "Resolution 2684 (2023)", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Adopted by the Security Council at its 9335th meeting, on 2 June 2023", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Security Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Recalling its resolution 1970 (2011) imposing the arms embargo on Libya and all its subsequent relevant resolutions,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": [["1970 (2011)", "https://undocs.org/en/S/RES/1970(2011)"]]}, {"text": "Recalling its resolutions 2292 (2016), 2357 (2017), 2420 (2018), 2473 (2019), 2526 (2020), 2578 (2021) and 2635 (2022) concerning the strict implementation of the arms embargo on the high seas off the coast of Libya,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recalling", "action_verb_type": "preambular", "action_verb_normalized": "recall", "links": [["2292 (2016)", "https://undocs.org/en/S/RES/2292(2016)"], ["2357 (2017)", "https://undocs.org/en/S/RES/2357(2017)"], ["2420 (2018)", "https://undocs.org/en/S/RES/2420(2018)"], ["2473 (2019)", "https://undocs.org/en/S/RES/2473(2019)"], ["2526 (2020)", "https://undocs.org/en/S/RES/2526(2020)"], ["2578 (2021)", "https://undocs.org/en/S/RES/2578(2021)"], ["2635 (2022)", "https://undocs.org/en/S/RES/2635(2022)"]]}, {"text": "Reaffirming its resolution 2656 (2022),", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": [["2656 (2022)", "https://undocs.org/en/S/RES/2656(2022)"]]}, {"text": "Recognizing the important role of neighbouring countries and regional organizations,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Recognizing", "action_verb_type": "preambular", "action_verb_normalized": "recogniz", "links": []}, {"text": "Mindful of its primary responsibility for the maintenance of international peace and security under the Charter of the United Nations,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Reaffirming its determination that terrorism, in all forms and manifestations, constitutes one of the most serious threats to peace and security,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": []}, {"text": "Acting under Chapter VII of the Charter of the United Nations,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Acting", "action_verb_type": "preambular", "action_verb_normalized": "act", "links": []}, {"text": "Decides to extend the authorizations as set out in resolution 2635 (2022) for a further 12 months from the date of this resolution;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": [["2635 (2022)", "https://undocs.org/en/S/RES/2635(2022)"]]}, {"text": "Requests the Secretary-General to report to the Security Council within six months and eleven months of the adoption of this resolution on its implementation;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "Decides to remain actively seized of the matter.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}]