[{"text": "Human Rights Council", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Fifty-sixth session", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "18 Juneâ€“12 July 2024", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Agenda item 10", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Technical assistance and capacity-building", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Resolution adopted by the Human Rights Council \non 11 July 2024", "type": "frontmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "56/16.\tTechnical assistance and capacity-building to improve human rights in Libya", "type": "title", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "The Human Rights Council,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "Guided by the purposes and principles of the Charter of the United Nations,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Guided", "action_verb_type": "preambular", "action_verb_normalized": "guided", "links": []}, {"text": "Reaffirming the Universal Declaration of Human Rights and relevant international human rights treaties,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": []}, {"text": "Confirming the primary responsibility of States to promote and protect human rights,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Confirming", "action_verb_type": "preambular", "action_verb_normalized": "confirm", "links": []}, {"text": "Reaffirming its strong commitment to the sovereignty, independence, unity and territorial integrity of Libya,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Reaffirming", "action_verb_type": "preambular", "action_verb_normalized": "reaffirm", "links": []}, {"text": "Commending the continued positive cooperation and constructive engagement of the Libyan authorities with the Office of the United Nations High Commissioner for Human Rights, and noting with appreciation the ongoing work of the Office of the High Commissioner in providing technical assistance and capacity-building to Libya,", "type": "paragraph", "heading_level": null, "paragraph_type": "preambular", "paragraph_level": null, "prefix": null, "action_verb": "Commending", "action_verb_type": "preambular", "action_verb_normalized": "commend", "links": []}, {"text": "Requests the Office of the United Nations High Commissioner for Human Rights to continue to provide technical assistance and capacity-building to Libya as provided for by the Human Rights Council in its resolution 52/41 of 4 April 2023;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "1.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "Also requests the Office of the High Commissioner to present to the Human Rights Council at its sixtieth session a report on the implementation of the present resolution, to be followed by an interactive dialogue;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "2.", "action_verb": "requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "Invites the Office of the High Commissioner to work closely with the Libyan authorities, the United Nations Support Mission in Libya, the African Union and all other relevant regional and international organizations;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "3.", "action_verb": "Invites", "action_verb_type": "operative", "action_verb_normalized": "invit", "links": []}, {"text": "Requests the Secretary-General to provide the Office of the High Commissioner with the resources necessary for the full implementation of the present resolution;", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "4.", "action_verb": "Requests", "action_verb_type": "operative", "action_verb_normalized": "request", "links": []}, {"text": "Decides to remain seized of the matter.", "type": "paragraph", "heading_level": null, "paragraph_type": "operative", "paragraph_level": 1, "prefix": "5.", "action_verb": "Decides", "action_verb_type": "operative", "action_verb_normalized": "decid", "links": []}, {"text": "35th meeting", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "11 July 2024", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}, {"text": "[Adopted without a vote.]", "type": "backmatter", "heading_level": null, "paragraph_type": null, "paragraph_level": null, "prefix": null, "action_verb": null, "action_verb_type": null, "action_verb_normalized": null, "links": []}]